{
    "name": "banglipai/utility",
    "description": "Utility Laravel Starter",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BangLipai\\Utility\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "banglipai\\ServiceProvider",
                "banglipai\\MigrationServiceProvider",
                "banglipai\\ResponseServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "BangLipai",
            "email": "developer.kukuh@gmail.com"
        }
    ],
    "require": {
        "illuminate/database": "8.12|^9.0|^10.0|^11.0",
        "illuminate/support": "8.12|^9.0|^10.0|^11.0",
        "illuminate/container": "8.12|^9.0|^10.0|^11.0"
    }
}
